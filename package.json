{
  "name": "huiyuan_private_assets",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port=3000",
    "build": "pnpm lint && vite build",
    "preview": "vite preview",
    "dev:docs": "pnpm --filter docs dev",
    "build:docs": "pnpm --filter docs build",
    "preview:docs": "pnpm --filter docs preview",
    "build:package": "pnpm --filter ./packages/* build",
    "lint": "vue-tsc --noEmit && eslint .",
    "lint:fix": "eslint --fix .",
    "lint:check": "pnpm --filter eslint-config run check",
    "lint:report": "pnpm --filter eslint-config run report",
    "postinstall": "pnpm build:package",
    "commit:.": "git pull && git add . && gitmoji -c && git push",
    "commit": "git pull && gitmoji -c"
  },
  "dependencies": {
    "vue": "catalog:"
  },
  "devDependencies": {
    "@hyuan/browser-data-structure": "workspace:",
    "@hyuan/chalk": "workspace:",
    "@hyuan/eslint-config": "workspace:",
    "@hyuan/typescript-config": "workspace:",
    "eslint": "catalog:",
    "gitmoji-cli": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vue-tsc": "catalog:"
  },
  "author": "刘汇源",
  "license": "ISC"
}
